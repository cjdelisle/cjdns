import json
import cjdnsadmin.graphMaker as graph
import networkx as nx
from networkx.readwrite import json_graph

G=graph.makeGraph()
d=json_graph.node_link_data(G)

print json.dumps(d,sort_keys=True,indent=4,separators=(',', ': '))
